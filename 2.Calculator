#include<iostream>
using namespace std;
int main()
{
    float x,y;
    char ch;
    cin>>x>>ch>>y;
    switch(ch)
    {
        case '+':
            cout<<"Addition = "<<x+y;
            break;
        case '-':
            cout<<"Substraction = "<<x-y;
            break;
        case '*':
            cout<<"Multiplication = "<<x*y ;
            break;
        case '/':
            cout<<"Division = "<<x/y ;
            break;      
    }
}
