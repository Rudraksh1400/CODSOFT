#include<iostream>
#include<vector>
#include<string>
using namespace std;
void par(int n,int idx,string s)
{
    if(idx==2*n)
    {
        int c1 = 0;
        int c2 = 0;
        for(int i=0;i<n*2;i++)
        {
            if(s[i]=='(') c1++;
            if(s[i]==')') c2++;
            if(c2>c1) return;
        }
        if(c1>n || c2>n) return;
        if(s[n*2-1]=='(') return;
        cout<<s<<endl;
        return;
    }
    if(idx<n*2)
    {
        s.push_back('(');
        par(n,idx+1,s);
        s.pop_back();
        s.push_back(')');
        par(n,idx+1,s);
    }
}
int main()
{
    int n = 4;
    string s;
    s.push_back('(');
    par(n,1,s);
}
